{
    "basic": {
        "account_name": "BtbN",
        "api_type": "github",
        "project_name": "FFmpeg-Builds"
    },
    "build": {
        "no_pull": true
    },
    "decompress": {
        "single_dir": true
    },
    "download": {
        "add_version_to_filename": true,
        "exclude_keyword": [
            "n4",
            "lgpl",
            "shared"
        ],
        "filetype": ["zip","tar.xz"],
        "keyword": [
            "%OS",
            "%arch",
            "master"
        ]
    },
    "process": {
        "image_name": "ffmpeg"
    },
    "jsonver": "1.0.1"
}